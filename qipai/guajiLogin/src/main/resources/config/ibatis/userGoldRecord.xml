<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap        
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"        
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="userGoldRecord">

	<insert id="save_user_gold_record" parameterClass="com.sy.sanguo.game.bean.UserGoldRecord">
		INSERT INTO user_gold_record$userSeq$
		(userId,freeGold,gold,addFreeGold,addGold,recordType,playType,sourceType,createTime)
		values
		(#userId#,#freeGold#,#gold#,#addFreeGold#,#addGold#,#recordType#,#playType#,#sourceType#,#createTime#)
	</insert>

</sqlMap>