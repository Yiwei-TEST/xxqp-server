<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap        
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"        
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="userGoldBeanRecord">

	<insert id="save_user_gold_bean_record" parameterClass="com.sy.sanguo.game.bean.UserGoldBeanRecord">
		INSERT INTO user_gold_bean_record
		(userId,goldBean,addGoldBean,recordType,playType,sourceType,createTime)
		values
		(#userId#,#goldBean#,#addGoldBean#,#recordType#,#playType#,#sourceType#,#createTime#)
	</insert>

</sqlMap>